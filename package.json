{
  "name": "react-component-boilerplate",
  "description": "A boilerplate to create React components using react-transform-hmr, Babel 6, webpack and SASS",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint components test examples",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "prepublish": "npm run lint && npm run test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "dev":"node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ritz078/react-component-boilerplate.git"
  },
  "keywords": [
    "react-component-keywords"
  ],
  "author": "Ritesh Kumar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritz078/react-component-boilerplate/issues"
  },
  "homepage": "https://github.com/ritz078/react-component-boilerplate",
  "devDependencies": {
    "babel": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "commitizen": "^2.5.0",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-react": "^2.3.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "isparta": "^4.0.0",
    "mocha": "^2.2.5",
    "node-sass": "^3.4.2",
    "rimraf": "^2.3.4",
    "sass-loader": "^3.1.2",
    "semantic-release": "^4.3.5",
    "style-loader": "^0.13.0",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "invariant": "^2.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
